<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- アプリ名指定 -->
    <!-- Debug:3階層上フォルダ名称 -->
    <add key="AppName" value="Debug"/>
    <!-- アプリ設定 -->
    <!-- フォーム位置(左上) -->
    <add key="DefaultLocationX" value="0"/>
    <add key="DefaultLocationY" value="0"/>
    <!-- 初期フォームサイズ -->
    <add key="DefaultWidth" value="1000"/>
    <add key="DefaultHeight" value="1000"/>
    <!-- 移動距離 -->
    <add key="UpMoveDistance" value="100"/>
    <add key="DownMoveDistance" value="100"/>
    <add key="RightMoveDistance" value="100"/>
    <add key="LeftMoveDistance" value="100"/>
    <!-- ズーム倍率 -->
    <add key="ZoomInRatio" value="2.0"/>
    <add key="ZoomOutRatio" value="0.5"/>
    <!-- 初期表示ズーム倍率 -->
    <add key="DefaultZoomRatio" value="0.5"/>
    <!--
    機能ボタン(ズームと次・前)指定
    Ctrl、Shift
    -->
    <add key="FunctionKey" value="ctrl"/>
  </appSettings>
</configuration>