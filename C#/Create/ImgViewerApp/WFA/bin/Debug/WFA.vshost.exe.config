<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- アプリ名指定 -->
    <!-- Debug:3階層上フォルダ名称 -->
    <add key="AppName" value="Debug"/>
    <!-- アプリ設定 -->
    <!-- 右移動距離 -->
    <add key="Hoge01" value="hogehoge"/>
    <!-- ズーム倍率 -->
    <add key="ZoomInRatio" value="2.0"/>
    <add key="ZoomOutRatio" value="0.5"/>
    <!-- 初期表示ズーム倍率 -->
    <add key="DefaultZoomRatio" value="0.5"/>
    <!--
    機能ボタン(ズームと次・前)指定
    Ctrl、Shift
    -->
    <add key="FunctionKey" value="ctrl"/>
  </appSettings>
</configuration>