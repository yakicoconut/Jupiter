<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- 詳細ログ出力フラグ(True:出力する、False:出力しない) -->
		<!-- コピー用詳細ログ出力フラグ -->
		<add key="IsOutDetailLogCopy" value="True"/>
		<!-- 移動用詳細ログ出力フラグ -->
		<add key="IsOutDetailLogMove" value="True"/>

		<!-- 詳細ログファイル出力フォルダパス -->
		<add key="DetailLogOutPath" value=".\"/>

		<!-- パスリスト
           ・「,」区切り複数指定可
           ・「収集対象パスリスト」と「収集先パスリスト」の内容は対とすること
           ・対象ファイルフィルタは対象フォルダの後にスペース区切りで記載
           ・「[YYYYMMDD]」は実行時に当日日付に置き換える
        -->
		<!-- コピー用収集対象パスリスト -->
		<add key="ColSrcPathAryCopy" value="
D:\Dtemp\Test01 *[YYYYMMDD]*.log,
D:\Dtemp\Test02 *[YYYYMMDD]*.log"/>
		<!-- コピー用収集先パスリスト -->
		<add key="ColDstPathAryCopy" value="
TestLog\Test01,
TestLog\Test02"/>

		<!-- 移動用収集対象パスリスト -->
		<add key="ColSrcPathAryMove" value="
D:\Dtemp\Test01,
D:\Dtemp\Test02"/>
		<!-- 移動用収集先パスリスト -->
		<add key="ColDstPathAryMove" value="
TestLog\Test01,
TestLog\Test02"/>

		<!-- robocopyオプション
             /S  :サブディレクトリは対象、空のディレクトリは非対象
             /E  :空のディレクトリを含むサブディレクトリを対象
             /MOV:ファイル移動(コピー後にコピー元から削除)
            /MOVE:ファイルとディレクトリを移動(コピー後にコピー元から削除)
             /XX:コピー先にだけ存在するファイルとディレクトリを除外
             /IS :同一ファイルを含む
         /XF file:指定された名前/パス/ワイルドカードに一致するファイルを除外
             /R:n:失敗したコピーに対する再試行数:既定値は1,000,000回
             /W:n:再試行と再試行間の待機時間:既定値は30秒
             /L  :リストのみ - いずれのファイルにもコピー、タイムスタンプの追加、
                  または削除は実施しない
        /MAXAGE:n:「n」より古いファイルを除外
		          「n<1900」の場合は「n=n日」、それ以外は「n=YYYYMMDD」の日付
        /MINAGE:n:「n」より新しいファイルを除外
		           「n」の指定は「/MAXAGE」と同様
        -->
        <!-- 特殊文字列
               ・「[YYYYMMDD]」は実行時に当日日付に置き換える
        -->
		<!-- コピー用オプション -->
		<add key="RobocopyOptCopy" value="/E /xx /R:0 /MAXAGE:[YYYYMMDD]"/>
		<!-- 移動用オプション -->
		<add key="RobocopyOptMove" value="/E /xx /R:0 /MOV /IS /XF *[YYYYMMDD]*.log *ERR99999999999.log"/>
	</appSettings>
</configuration>