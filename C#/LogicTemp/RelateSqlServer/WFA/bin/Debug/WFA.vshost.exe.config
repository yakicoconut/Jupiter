<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- アプリ名指定 -->
    <!-- Debug:3階層上フォルダ名称 -->
    <add key="AppName" value="Debug_"/>
    <!-- アプリ設定 -->
    <!--ローカルサーバ名称 -->
    <add key="LocalServer" value="(local)"/>
    <!-- ローカルDB名称 -->
    <add key="LocalDB" value="GemDB"/>
    <!-- 実行SQL -->
    <add key="ExecSqlScript" value="SELECT TOP 1 TransactionData FROM TransactionLog ORDER BY TransactionNo DESC"/>
    <!-- 出力ファイル名称(DateTime:日時分) --><!--
    <add key="OutFileName" value="DateTime"/>-->
  </appSettings>
</configuration>