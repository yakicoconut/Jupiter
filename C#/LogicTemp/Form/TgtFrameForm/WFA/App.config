<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- アプリ名指定 -->
    <!-- Debug:3階層上フォルダ名称 -->
    <add key="AppName" value="Debug"/>
    <!-- アプリ設定 -->
    <!-- 初期値 -->
    <!-- 左上X座標 -->
    <add key="LeftTopX" value="1"/>
    <!-- 左上Y座標 -->
    <add key="LeftTopY" value="2"/>
    <!-- 右下X座標 -->
    <add key="RightBottomX" value="3"/>
    <!-- 右下X座標 -->
    <add key="RightBottomY" value="4"/>
    <!-- 拡大倍率 -->
    <add key="ZoomRatio" value="1.00"/>
    <!-- 対象画面範囲取得フォーム不透明度設定 -->
    <add key="TgtFrameOpacity" value="0.10"/>
    <!-- キャプチャ保存フォルダ名称 -->
    <add key="CaptureDirName" value="CaptureImg"/>
    <!-- キャプチャファイル名(ファイル名後に「年月日時分秒ミリ」自動付加) -->
    <add key="CaptureFileName" value="CaptureImg_"/>
    <!-- 移動距離 -->
    <add key="MoveDist" value="1"/>
  </appSettings>
</configuration>